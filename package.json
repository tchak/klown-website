    "@remix-run/react": "^0.21.0",
    "@urql/core": "^2.3.4",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "history": "^5.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.2",
    "rehype-react": "^6.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remix": "^0.21.0",
