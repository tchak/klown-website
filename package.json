    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/siema": "^1.4.7",
    "concurrently": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.4",
    "typescript": "^4.5.2"
