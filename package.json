{
  "private": true,
  "name": "klown",
  "description": "",
  "license": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:start": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "sass -w styles/main.scss app/styles/index.css",
    "build": "cross-env NODE_ENV=production run-s build:*",
    "build:clean": "del .cache netlify/functions/server/build public/build app/styles",
    "build:css": "sass styles/main.scss app/styles/index.css",
    "build:graphql": "graphql-codegen -r dotenv/config --config codegen.yml",
    "build:remix": "remix build",
    "lint": "run-s lint:*",
    "lint:types": "tsc",
    "lint:js": "eslint . --ext .ts,.tsx",
    "size": "size-limit",
    "start": "remix-serve ./netlify/functions/server/build",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@react-aria/interactions": "^3.7.0",
    "@remix-run/dev": "^1.1.1",
    "@remix-run/netlify": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@urql/core": "^2.3.6",
    "dotenv": "^10.0.0",
    "graphql": "^16.2.0",
    "history": "^5.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.2.1",
    "rehype-react": "^6.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remix": "^1.1.1",
    "siema": "^1.5.1",
    "unified": "^9.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.3.2",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typed-document-node": "^2.2.2",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@remix-run/dev": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@size-limit/preset-app": "^7.0.4",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/siema": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typestrict": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "size-limit": "^7.0.4",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "size-limit": [
    {
      "path": "public/build/**/*.js"
    }
  ],
  "prettier": {
    "singleQuote": true
  }
}
