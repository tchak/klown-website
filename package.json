  "scripts": {
    "typecheck": "tsc",
    "build": "run-s build:css build:remix",
    "build:remix": "remix build",
    "start:dev": "remix dev",
    "dev": "concurrently \"npm:watch:css\" \"npm:start:dev\"",
    "netlify:dev": "netlify dev",
    "build:css": "sass styles/main.scss app/styles/index.css",
    "watch:css": "sass -w styles/main.scss app/styles/index.css",
    "test": "run-s typecheck",
    "generate": "graphql-codegen -r dotenv/config --config codegen.yml",
    "postinstall": "remix setup"
  },
  "dependencies": {
    "@netlify/functions": "^0.7.2",
    "@react-aria/interactions": "^3.6.0",
    "@remix-run/dev": "^0.20.1",
    "@remix-run/netlify": "^0.20.1",
    "@remix-run/react": "^0.20.1",
    "@urql/core": "^2.3.4",
    "dotenv": "^10.0.0",
    "graphql": "^15.6.0",
    "history": "^5.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^6.0.0",
    "remix": "^0.20.1",
    "siema": "^1.5.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typed-document-node": "^2.2.0",
    "@graphql-codegen/typescript": "^2.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@remix-run/serve": "^0.20.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/siema": "^1.4.7",
    "concurrently": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.43.4",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
